{
  "name": "graphql-cli-maana",
  "version": "3.2.12",
  "description": "Maana plugin for graphql-cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/maana-io/q-cli"
  },
  "main": "./dist/index.js",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "prettier": "./node_modules/.bin/prettier --write \"src/**/*.{js,jsx,json,css,gql,graphql}\"",
    "copy-scripts": "cp -r ./src/scripts dist/.",
    "build": "rm -rf ./dist && ./node_modules/.bin/babel  ./src -d ./dist && npm run copy-scripts"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,gql,graphql}": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "dependencies": {
    "adm-zip": "0.4.14",
    "chalk": "^2.4.1",
    "command-exists": "^1.2.7",
    "cross-spawn": "^6.0.5",
    "fast-glob": "^2.2.2",
    "fs-extra": "^7.0.0",
    "graphql": "^14.1.1",
    "graphql-config": "^2.2.1",
    "graphql-request": "^1.8.2",
    "graphql-tag": "^2.9.2",
    "inquirer": "^6.3.1",
    "js-yaml": "^3.12.0",
    "md5-base64": "0.0.3",
    "merge-graphql-schemas": "^1.5.7",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.2",
    "n3": "^1.0.0-beta.2",
    "os": "^0.1.1",
    "papaparse": "^4.6.0",
    "parse-github-url": "^1.0.2",
    "querystring": "^0.2.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "rimraf": "^2.6.2",
    "shelljs": "^0.8.3",
    "strip-bom": "^4.0.0",
    "tmp": "0.0.33"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-decorators": "7.3.0",
    "@babel/plugin-proposal-do-expressions": "7.2.0",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-function-bind": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.2.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.2.0",
    "@babel/plugin-proposal-numeric-separator": "7.0.0",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "7.3.2",
    "@babel/plugin-proposal-throw-expressions": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-transform-arrow-functions": "^7.0.0-beta.56",
    "@babel/plugin-transform-async-to-generator": "7.1.0",
    "@babel/preset-env": "^7.1.0",
    "eslint": "^5.6.0",
    "lint-staged": "^8.1.4",
    "prettier": "^1.14.3",
    "webpack": "^4.20.2"
  }
}
